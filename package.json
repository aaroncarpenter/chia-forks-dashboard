{
  "name": "ForkBoard",
  "productName": "ForkBoard",
  "description": "An ElectronJS Dashboard App to show wallets balances from Chia and Forks in one convenient location.",
  "keywords": [],
  "main": "./src/main.js",
  "version": "1.1.1",
  "author": "Aaron Carpenter",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . ForkBoard --overwrite --win32 --x64 --icon=assets/icons/fork-board-gray.ico --prune=true --out=./out/win --ignore=^/assets/config --ignore=^/logs",
    "installer-win": "electron-installer-windows --src ./out/win/ForkBoard-win32-x64 --dest ./out/win/ForkBoard-%npm_package_version%-x64 --icon=assets/icons/fork-board-gray.ico",
    "package-mac-arm64": "electron-packager . ForkBoard --darwin --arm64 --out=./out/arm64/ --icon=assets/icons/fork-board-gray.icns --overwrite=true --prune=true --ignore=^/assets/config --ignore=^/logs",
    "installer-mac-arm64": "electron-installer-dmg out/arm64/ForkBoard-darwin-x64/ForkBoard.app out/arm64/ForkBoard-arm64",
    "package-mac-x64": "electron-packager . ForkBoard --darwin --x64 --out=./out/x64/ --icon=assets/icons/fork-board-gray.icns --overwrite=true --prune=true --ignore=^/assets/config --ignore=^/logs",
    "installer-mac-x64": "electron-installer-dmg out/x64/ForkBoard-darwin-x64/ForkBoard.app out/x64/ForkBoard-x64",
    "package-deb": "electron-packager . ForkBoard --overwrite --linux --x64 --icon=assets/icons/fork-board-gray.png --prune=true --out=./out/deb --ignore=^/assets/config --ignore=^/logs",
    "installer-deb": "electron-installer-debian --src ./out/deb/ForkBoard-linux-x64 --dest ./out/deb/ForkBoard-$npm_package_version-x64 --arch amd64 --icon=assets/icons/fork-board-gray.png  --overwrite=true"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "chart.js": "^3.9.1",
    "electron": "^15.3.0",
    "electron-installer-windows": "^3.0.0",
    "electron-log": "^4.4.1",
    "electron-packager": "^15.4.0",
    "electron-prompt": "^1.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "electron": "^15.3.0"
  }
}
